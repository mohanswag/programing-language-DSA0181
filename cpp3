#include <iostream>
using namespace std;

class R192210061 {
public:
    int calculateSum(int arr[], int size) {
        int sum = 0;
        for (int i = 0; i < size; i++) {
            sum += arr[i];
        }
        return sum;
    }
};

int main() {
    int arr[] = {5, 10, 15, 20};
    int size = sizeof(arr) / sizeof(arr[0]);
    R192210061 obj;
    cout << "Sum of elements: " << obj.calculateSum(arr, size) << endl;
    return 0;
}
